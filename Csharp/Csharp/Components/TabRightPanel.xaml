<UserControl x:Class="Csharp.Components.TabRightPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Csharp.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="12" Background="LightGray">
        <TabControl Background="LightGray">
            <TabItem Header="Навыки">
                <local:TabSkills></local:TabSkills>
            </TabItem>
            <TabItem Header="Способности">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Снаряжение">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Уровни">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="БИО">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Кубы">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
