<UserControl x:Class="Csharp.Components.Skills"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Csharp.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Name="Vnimatelnost"  Margin="10,0,10,0" Background="White">
        <Grid>
            <Button  Margin="5" Height="10" Width="10">
            </Button>
        </Grid>
        <Label Background="White" Width="130" VerticalAlignment="Center" 
                   Content="{Binding SkillName}"/>
        <Label VerticalAlignment="Center" Width="38"
                   Content="{Binding Value}" HorizontalAlignment="Right" UseLayoutRounding="False"/>
    </DockPanel>
</UserControl>
