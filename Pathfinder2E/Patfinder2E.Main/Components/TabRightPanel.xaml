<UserControl x:Class="Pathfinder2E.Shell.Components.TabRightPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Pathfinder2E.Shell.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="12" Background="LightGray">
        <TabControl Background="LightGray">
            <TabItem Header="Навыки">
                <local:TabSkills/>
            </TabItem>
            <TabItem Header="БИО">
                <local:TabBio/>
            </TabItem>
            <TabItem Header="Кубы">
                <local:TabDice/>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
